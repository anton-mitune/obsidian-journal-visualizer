{
	"name": "journal-visualizer",
	"version": "0.1.0",
	"description": "Turn your notes into insight. Get contextual stats, dashboards, and smart suggestions for your vault.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"generate-demo-vault": "node scripts/generate_demo_vault.js --out=./joviz-demo-vault",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"visualization",
		"analytics",
		"vault"
	],
	"author": "Anton Mitune (@anton-mitune)",
	"license": "Unlicense",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4",
		"minimist": "^1.2.8",
		"@faker-js/faker": "^8.0.2"
	},
	"dependencies": {}
}
